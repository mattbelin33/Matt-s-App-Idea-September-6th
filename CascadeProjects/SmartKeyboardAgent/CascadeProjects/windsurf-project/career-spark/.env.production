# Production Environment Variables for Career Spark
# Copy this to .env.local and fill in your actual API keys

# API Configuration (Server-side only - secure)
CAREER_SPARK_API_KEY=your-production-api-key-here
ADMIN_API_KEY=your-admin-key-here

# Rate Limiting (Production values)
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_REQUESTS_PER_HOUR=2000

# External APIs
QUOTABLE_API_URL=https://api.quotable.io

# App Configuration
NEXT_PUBLIC_APP_URL=https://career-spark-ai.netlify.app

# Public API Key (Client-side - this will be exposed)
NEXT_PUBLIC_API_KEY=your-public-api-key-here
